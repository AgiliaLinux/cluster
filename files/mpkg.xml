<?xml version="1.0" encoding="utf-8"?>
<mpkgconfig>
	<sys_cache>
		NODE/var/mpkg/cache/
	</sys_cache>
	<override_arch>
		ARCH
	</override_arch>
	<database_url>
		sqlite://NODE/var/mpkg/packages.db
	</database_url>
	<scripts_dir>
		NODE/var/mpkg/scripts/
	</scripts_dir>
	<run_scripts>
		yes
	</run_scripts>
	<checkFileConflicts>
		preinstall
	</checkFileConflicts>
	<sys_backup>
		NODE/var/mpkg/backup/
	</sys_backup>
	<sys_root>
		NODE/
	</sys_root>
	<disable_dependencies>
		no
	</disable_dependencies>
	<enable_download_resume>
		no
	</enable_download_resume>
	<enable_prelink>
		no
	</enable_prelink>
	<enable_prelink_randomization>
		no
	</enable_prelink_randomization>
	<enable_delta>
		no
	</enable_delta>
	<cdrom_device>
		/dev/sr0
	</cdrom_device>
	<cdrom_mountpoint>
		/var/log/mount/
	</cdrom_mountpoint>
	<build_pkg_type>
		txz
	</build_pkg_type>
</mpkgconfig>
